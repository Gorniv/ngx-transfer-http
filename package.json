{
  "name": "universal-demo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ssr": "npm run build:universal && npm run server",
    "ssr:debug": "ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true  && ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --app 1 && webpack && node server.js",
    "ssr:cw": "ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --watch",
    "ssr:sw": "ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --app 1 --watch",
    "ssr:webpack": "webpack --watch",
    "ssr:server": "nodemon server.js",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:universal": "ng build --prod && ng build --prod --app 1 && webpack --progress --colors",
    "server": "node server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "universal": "ng build --prod && ng build --prod --app 1 && ts-node server",
    "build:prerender": "npm run build:universal && npm run generate:prerender",
    "generate:prerender": "node prerender.js",
    "packagr": "ng-packagr -p lib/ngx-transfer-http/package.json",
    "prepublish:lib": "npm run packagr",
    "publish:lib": "npm publish ./lib/ngx-transfer-http/dist --access public"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "6.1.4",
    "@angular/common": "6.1.4",
    "@angular/compiler": "6.1.4",
    "@angular/core": "6.1.4",
    "@angular/forms": "6.1.4",
    "@angular/http": "6.1.4",
    "@angular/platform-browser": "6.1.4",
    "@angular/platform-browser-dynamic": "6.1.4",
    "@angular/router": "6.1.4",
    "@angular/service-worker": "6.1.4",
    "@nguniversal/common": "6.1.0",
    "@nguniversal/express-engine": "6.1.0",
    "@nguniversal/module-map-ngfactory-loader": "6.1.0",
    "@ngx-meta/core": "6.0.0",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "cookie-parser": "1.4.3",
    "core-js": "2.5.7",
    "ngx-cookie-service": "1.0.10",
    "node-fetch": "2.2.0",
    "rxjs": "6.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.7.5",
    "@angular/cli": "6.1.5",
    "@angular/compiler-cli": "6.1.4",
    "@angular/language-service": "6.1.4",
    "@angular/platform-server": "6.1.4",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "10.9.2",
    "codelyzer": "4.4.4",
    "jasmine-core": "3.2.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "2.0.2",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.3.1",
    "nodemon": "1.18.3",
    "preboot": "6.0.0-beta.5",
    "protractor": "5.4.0",
    "ssri": "6.0.0",
    "tslint": "5.11.0",
    "typescript": "2.9.2",
    "ng-packagr": "4.1.0"
  }
}
