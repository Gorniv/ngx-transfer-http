{
  "name": "universal-demo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ssr": "npm run build:universal && npm run server",
    "ssr:debug": "ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true  && ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --app 1 && webpack && node server.js",
    "ssr:cw": "ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --watch",
    "ssr:sw": "ng build --dev --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --app 1 --watch",
    "ssr:webpack": "webpack --watch",
    "ssr:server": "nodemon server.js",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:universal": "ng build --prod && ng build --prod --app 1 && webpack --progress --colors",
    "server": "node server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "universal": "ng build --prod && ng build --prod --app 1 && ts-node server",
    "build:prerender": "npm run build:universal && npm run generate:prerender",
    "generate:prerender": "node prerender.js",
    "packagr": "ng-packagr -p lib/ngx-transfer-http/package.json",
    "prepublish:lib": "npm run packagr",
    "publish:lib": "npm publish ./lib/ngx-transfer-http/dist --access public"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "5.2.9",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/router": "5.2.9",
    "@angular/service-worker": "5.2.9",
    "@nguniversal/common": "5.0.0",
    "@nguniversal/express-engine": "5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "5.0.0",
    "@ngx-meta/core": "5.0.0",
    "@ngx-translate/core": "9.1.1",
    "@ngx-translate/http-loader": "2.0.1",
    "cookie-parser": "1.4.3",
    "core-js": "2.5.4",
    "ngx-cookie-service": "1.0.10",
    "node-fetch": "2.1.2",
    "rxjs": "5.5.8",
    "zone.js": "0.8.24"
  },
  "devDependencies": {
    "@angular/cli": "1.7.3",
    "@angular/compiler-cli": "5.2.9",
    "@angular/language-service": "5.2.9",
    "@angular/platform-server": "5.2.9",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "9.6.1",
    "codelyzer": "4.2.1",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "1.0.0",
    "nodemon": "1.17.3",
    "preboot": "6.0.0-beta.3",
    "protractor": "5.3.0",
    "ssri": "5.3.0",
    "tslint": "5.9.1",
    "typescript": "2.6.2",
    "ng-packagr":"2.4.5"
  }
}
