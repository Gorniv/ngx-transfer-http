{
    "printWidth": 100,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "arrowParens": "always",
    "overrides": [
      {
        "files": "*.ts",
        "options": {
          "parser": "typescript"
        }
      },
      {
        "files": "*.json",
        "options": {
          "parser": "json",
          "trailingComma": "none"
        }
      },
      {
        "files": "*.scss",
        "options": {
          "parser": "scss",
          "trailingComma": "none"
        }
      },
      {
        "files": "*.css",
        "options": {
          "parser": "css",
          "trailingComma": "none"
        }
      }
    ]
  }
  